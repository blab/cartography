<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

        <replicates>1</replicates>

        <simulation>

            <genome>
                <length>21285</length>
	            <sequences>
	            
	            </sequences>
	            <feature>
		            <name>CDS</name>
		            <type>aminoAcid</type>
		            <coordinates>1-21285</coordinates>
	            </feature>
           </genome>

            <population>
                <populationSize>10000</populationSize>
	            <inoculum>all</inoculum>
            </population>

		    <populationType>staticPopulation</populationType>

            <fitnessFunction>
                <!-- Replicase is under purifying selection. -->
                <purifyingFitness>
	                <feature>CDS</feature>
                 	<rank>
                		<order>chemical</order>
                		<breakTies>random</breakTies>
                		<probableSet>5</probableSet>
                	</rank>
                	<fitness>
                		<lowFitness>0.8</lowFitness>
		                <minimumFitness>0.7</minimumFitness>
                	</fitness>
                	<fluctuate>
                		<rate>0.00001</rate>
                		<fitnessLimit>0.8</fitnessLimit>
                	</fluctuate>
                </purifyingFitness>
            </fitnessFunction>

	        <mutator>
	            <nucleotideMutator>
	                <mutationRate>1E-4</mutationRate>
	                <transitionBias>2.0</transitionBias>
	            </nucleotideMutator>
	        </mutator>

	        <replicator>
	            <recombinantReplicator>
					<dualInfectionProbability>0.1</dualInfectionProbability>
					<recombinationProbability>0.0067</recombinationProbability>
				</recombinantReplicator>
	        </replicator>

            <epoch>
                <name>epoch1</name>
                <generationCount>1800</generationCount>
            </epoch>

            <samplingSchedule>
                <sampler>
                    <atFrequency>1</atFrequency>
                    <fileName>simulated_sequences.fasta</fileName>
                    <alignment>
                    	<sampleSize>15</sampleSize>
	                    <format>FASTA</format>
                        <label>sample_%g_%s|%g|%f|%c</label>
                    </alignment>
                </sampler>
            </samplingSchedule>

            <eventLogger>
                <fileName>events.txt</fileName>
            </eventLogger>
        </simulation>
</santa>
