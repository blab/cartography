        """

rule seasonal_flu_training_create_distance_matrix:
    message: "creating the distance matrix to be used in the rest of the analysis"
    input:
        alignment = "seasonal-flu-nextstrain/results/aligned_{ancestral}.fasta"
    output:
        output = "seasonal-flu-nextstrain/results/distance_matrix_{ancestral}.csv"
    conda: "../cartography.yml"
    shell:
        """
        python3 notebooks/scripts/hamming_distance_from_fasta.py \
            --alignment {input.alignment} \
            --output {output.output}
        """

rule seasonal_flu_training_cluster_by_parameters:
    input:
        alignment=ancient("seasonal-flu-nextstrain/results/aligned_sequences.fasta"),
        distance_matrix=ancient("seasonal-flu-nextstrain/results/distance_matrix_sequences.csv"),
        clades=ancient("seasonal-flu-nextstrain/results/clades.json"),
    output:
        table=f"seasonal-flu-nextstrain/results/gridsearch/{SEASONAL_FLU_METHOD_PARAMETERS.wildcard_pattern}.tsv",
    params:
        method_parameters=SEASONAL_FLU_METHOD_PARAMETERS.instance,
    conda: "../cartography.yml"
    script:
        "notebooks/scripts/cluster_by_parameters.py"


rule seasonal_flu_training_aggregate_clusters_by_parameters:
    input:
        tables=expand("seasonal-flu-nextstrain/results/gridsearch/{params}.tsv", params=SEASONAL_FLU_METHOD_PARAMETERS.instance_patterns)
    output:
        table="seasonal-flu-nextstrain/results/gridsearch.tsv"
    conda: "../cartography.yml"
    shell:
        """
        python3 notebooks/scripts/concatenate_tables.py \
            --tables {input.tables} \
            --output {output.table}
        """

rule seasonal_flu_training_output_grid_search:
    input:
