        mds_parameters = "seasonal-flu-nextstrain/results/mds_parameters.csv",
        tsne_parameters = "seasonal-flu-nextstrain/results/t-sne_parameters.csv",
        umap_parameters = "seasonal-flu-nextstrain/results/umap_parameters.csv",
        mcc_by_tsne_parameters = "seasonal-flu-nextstrain/results/mcc_by_t-sne_parameters.pdf",
        mcc_by_umap_parameters = "seasonal-flu-nextstrain/results/mcc_by_umap_parameters.pdf",
        mcc_by_mds_parameters = "seasonal-flu-nextstrain/results/mcc_by_mds_parameters.pdf",
        mcc_by_method_and_distance_threshold = "seasonal-flu-nextstrain/results/mcc_by_method_and_distance_threshold.pdf"
    conda: "../cartography.yml"
    log:
        "logs/seasonal-flu-nextstrain/summarize-grid-search.ipynb"
    notebook:
        "notebooks/2021-06-23-summarize-grid-search.ipynb"

rule seasonal_flu_training_embed_pca:
    message: "Creating the embedding for PCA"
    input:
        alignment = "seasonal-flu-nextstrain/results/aligned_{ancestral}.fasta",
        cluster = "seasonal-flu-nextstrain/results/pca_parameters.csv",
    output:
        dataframe = "seasonal-flu-nextstrain/results/embed_pca_{ancestral}.csv",
        figure = "seasonal-flu-nextstrain/results/embed_pca_{ancestral}.pdf",
        explained_variance = "seasonal-flu-nextstrain/results/explained_variance_pca_{ancestral}.csv"
    params:
        random_seed = RANDOM_SEED,
    conda: "../cartography.yml"
    shell:
        """
        embed \
            --alignment {input.alignment} \
            --cluster-data {input.cluster} \
            --random-seed {params.random_seed} \
            --output-dataframe {output.dataframe} \
            --output-figure {output.figure} \
            pca \
            --explained-variance {output.explained_variance}
        """

