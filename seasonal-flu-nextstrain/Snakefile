rule seasonal_flu_training_create_notebook_docs:
    message: "creating linked and grouped charts using the jupyter notebook"
    input:
        colors = "notebooks/config/color_schemes.tsv",
        #Charts, tree:
        node_df = "seasonal-flu-nextstrain/results/table.tsv",

        pca_df = "seasonal-flu-nextstrain/results/embed_pca_sequences.csv",
        explained_variance_pca = "seasonal-flu-nextstrain/results/explained_variance_pca_sequences.csv",

        mds_df = "seasonal-flu-nextstrain/results/embed_mds_sequences.csv",
        tsne_df = "seasonal-flu-nextstrain/results/embed_t-sne_sequences.csv",
        umap_df = "seasonal-flu-nextstrain/results/embed_umap_sequences.csv",

        #Scatterplot:
        scatterplot_pca = "seasonal-flu-nextstrain/results/scatterplot_pca.csv",
