    output:
        dataframe = "seasonal-flu-nextstrain/results/HDBSCAN_metadata_{method}.csv",
    conda: "../cartography.yml"
    params:
        method = "{method}"
    shell:
        """
        python3 notebooks/scripts/metadata_HDBSCAN.py \
            --method {params.method} \
            --embedding {input.embedding} \
            --metadata {input.metadata} \
            --cluster-data {input.cluster} \
            --output {output.dataframe}
        """

rule seasonal_flu_training_concat_HDBSCAN_table:
    message: "concatenating all the HDBSCAN data into one csv dataframe"
    input:
       dataframe_pca = "seasonal-flu-nextstrain/results/HDBSCAN_metadata_pca.csv",
       dataframe_mds = "seasonal-flu-nextstrain/results/HDBSCAN_metadata_mds.csv",
       dataframe_tsne = "seasonal-flu-nextstrain/results/HDBSCAN_metadata_t-sne.csv",
       dataframe_umap = "seasonal-flu-nextstrain/results/HDBSCAN_metadata_umap.csv",
    output:
        metadata = "seasonal-flu-nextstrain/results/full_HDBSCAN_metadata.csv"
    params:
        column = "MCC",
    conda: "../cartography.yml"
    shell:
        """
        python3 notebooks/scripts/concatenate_tables.py \
            --tables {input.dataframe_pca} {input.dataframe_mds} {input.dataframe_tsne} {input.dataframe_umap} \
            --separator ',' \
            --sort-by {params.column} \
            --output {output.metadata}
        """

